{
  "name": "shelfdb",
  "version": "0.1.0",
  "description": "A simple wrapper model for the pouchdb database",
  "main": "index.js",
  "scripts": {
    "browserify": "./node_modules/.bin/browserify tmp/suite.js -o tmp/tests.js --ignore-missing",
    "clean": "rm -rf tmp",
    "create-suite": "node ./test/utils/create-suite.js",
    "create-tmp": "mkdir -p tmp",
    "mocha": "./node_modules/.bin/mocha -r test/utils/setup.js -R tap test/*.test.js",
    "test": "./node_modules/.bin/testem"
  },
  "keywords": [
    "pouchdb",
    "db",
    "mongo"
  ],
  "author": "daniel.eissing@gmx.net",
  "browser": {
    "mkdirp": false,
    "express-pouchdb": false
  },
  "license": "MIT",
  "dependencies": {
    "lodash": "^3.0.0",
    "deep-equal": "^1.0.0",
    "detect-node": "^2.0.3",
    "express": "^4.11.2",
    "express-pouchdb": "^0.14.0",
    "mkdirp": "^0.5.0",
    "pouchdb": "^3.3.1",
    "q": "^1.1.2"
  },
  "devDependencies": {
    "browserify": "^8.1.3",
    "chai": "^1.10.0",
    "fs.extra": "^1.3.2",
    "leveldown": "^0.10.4",
    "memdown": "^1.0.0",
    "mocha": "^2.1.0",
    "mocha-qa": "^0.9.3",
    "testem": "^0.6.39"
  }
}
